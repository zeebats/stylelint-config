  "devDependencies": {
    "@zeebats/eslint-config": "1.0.0",
    "lint-staged": "12.1.3",
    "simple-git-hooks": "2.7.0",
    "tape": "5.3.2"
  },
  "peerDependencies": {
    "postcss-html": "^1.0.0",
    "stylelint": "^14.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0"
  }
}
