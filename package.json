		"update:interactive": "npx npm-check -uE"
	},
	"devDependencies": {
		"@zeebats/eslint-config": "1.3.1",
		"lint-staged": "13.1.0",
		"simple-git-hooks": "2.8.1",
		"tape": "5.6.3"
	},
	"peerDependencies": {
		"postcss-html": "^1.0.0",
		"stylelint": "^14.0.0",
		"stylelint-config-standard": "^28.0.0",
		"stylelint-order": "^5.0.0"
	}
}
