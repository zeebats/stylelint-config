{
  "name": "@zeebats/stylelint-config",
  "version": "1.0.0",
  "description": "Stylelint sharable config for Zeebats projects",
  "keywords": [
    "stylelint",
    "stylelintconfig",
    "postcss"
  ],
  "repository": "https://github.com/zeebats/stylelint-config",
  "license": "MIT",
  "author": "Zeebats",
  "main": "index.js",
  "files": [
    "index.js",
    "README.md"
  ],
