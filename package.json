{
  "name": "@zeebats/stylelint-config",
  "version": "1.0.0",
  "description": "Stylelint sharable config for Zeebats projects",
  "keywords": [
    "stylelint",
    "stylelintconfig",
    "postcss",
    "sass"
  ],
  "repository": "https://github.com/zeebats/stylelint-config",
  "license": "MIT",
  "author": "Zeebats",
  "main": "index.js",
  "files": [
    "index.js",
    "postcss.js",
    "README.md",
    "sass.js"
  ],
  "scripts": {
    "eslint": "eslint '**/*.js'",
    "eslint:fix": "npm run eslint -- --fix",
    "update:interactive": "npx npm-check -uE"
