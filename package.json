{
	"name": "@zeebats/stylelint-config",
	"version": "1.2.0-beta.4",
	"description": "Stylelint sharable config for Zeebats projects",
	"keywords": [
		"stylelint",
		"stylelint-config",
		"postcss"
	],
	"repository": "https://github.com/zeebats/stylelint-config",
	"license": "MIT",
	"author": "Zeebats",
	"main": "index.js",
	"bin": {
		"stylelint": "node_modules/stylelint/bin/stylelint.js"
	},
	"files": [
		"index.js"
	],
	"scripts": {
		"eslint": "eslint '**/*.js'",
		"eslint:fix": "npm run eslint -- --fix",
		"test": "tape tests/*.js"
	},
	"dependencies": {
		"stylelint": "15.6.2"
	},
	"devDependencies": {
		"@zeebats/eslint-config": "1.3.1",
		"lint-staged": "13.2.2",
		"simple-git-hooks": "2.8.1",
		"tape": "5.6.3"
	},
	"peerDependencies": {
		"postcss-html": "^1.5.0",
		"stylelint-config-standard": "^33.0.0",
		"stylelint-order": "^6.0.0"
	}
}
