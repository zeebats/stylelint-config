{
	"name": "@zeebats/stylelint-config",
	"version": "1.1.0",
	"description": "Stylelint sharable config for Zeebats projects",
	"keywords": [
		"stylelint",
		"stylelint-config",
		"postcss"
	],
	"repository": "https://github.com/zeebats/stylelint-config",
	"license": "MIT",
	"author": "Zeebats",
	"main": "index.js",
	"files": [
		"index.js"
	],
	"scripts": {
		"eslint": "eslint '**/*.js'",
		"eslint:fix": "npm run eslint -- --fix",
