{
  "name": "@zeebats/stylelint-config",
  "version": "1.0.0",
  "description": "Stylelint sharable config for Zeebats projects",
  "keywords": [
    "stylelint",
    "stylelintconfig",
    "postcss"
  ],
  "repository": "https://github.com/zeebats/stylelint-config",
  "license": "MIT",
  "author": "Zeebats",
  "main": "index.js",
  "files": [
    "index.js",
    "README.md"
  ],
  "scripts": {
    "eslint": "eslint '**/*.js'",
    "eslint:fix": "npm run eslint -- --fix",
    "test": "tape tests/*.js",
    "update:interactive": "npx npm-check -uE"
  },
  "devDependencies": {
    "@babel/core": "7.16.5",
    "@babel/eslint-parser": "7.16.5",
    "@zeebats/eslint-config": "1.0.0",
    "eslint": "8.5.0",
    "eslint-plugin-jsdoc": "37.4.0",
    "eslint-plugin-unicorn": "39.0.0",
    "lint-staged": "12.1.3",
    "postcss-html": "1.3.0",
    "simple-git-hooks": "2.7.0",
    "stylelint": "14.1.0",
    "stylelint-config-html": "1.0.0",
    "stylelint-config-standard": "24.0.0",
    "stylelint-order": "5.0.0",
    "tape": "5.3.2"
  },
  "peerDependencies": {
    "postcss-html": ">=1.0.0",
    "stylelint": ">=14.0.0",
    "stylelint-config-html": ">=1.0.0",
    "stylelint-config-standard": ">=23.0.0",
    "stylelint-order": ">=5.0.0"
  }
}
